{
  "expo": {
    "name": "my-expo-project",
    "slug": "my-expo-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/svg/add-line.svg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.my-expo-project.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {"softwareKeyboardLayoutMode": "pan"},
    "web": {
      "favicon": "./assets/img/favicon.png"
    },
    "expo": {
      "experiments": {
        "tsconfigPaths": true
      }
    }
  }
}
